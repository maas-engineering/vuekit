{
  "name": "nuxt",
  "version": "0.38.0",
  "author": "Robin Scholz <https://github.com/robinscholz>, Christoph Jeworutzki <https://github.com/ChristophJeworutzki>",
  "dependencies": {
    "@maas/vue-equipment/composables": "workspace:composables@*",
    "@maas/vue-equipment/plugins": "workspace:plugins@*",
    "@maas/vue-equipment/utils": "workspace:utils@*",
    "@nuxt/kit": "^3.14.159"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.6.0",
    "@nuxt/eslint-config": "^0.7.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.14.159",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "nuxt": "^3.13.1",
    "vitest": "^2.1.5"
  },
  "scripts": {
    "build": "nuxt-module-build --outDir ../../dist/nuxt && pnpm dlx nuxi prepare ./playground",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "type": "module",
  "sideEffects": false
}
